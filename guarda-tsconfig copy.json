{
  "compilerOptions": {
      "outDir": "./dist",
      "module": "commonjs",
      "target": "es2019",
      "esModuleInterop": true,
      "allowJs": true,
      "strictNullChecks": false,
      "rootDirs": ["src", "test"],
      "baseUrl": "src",
      "paths": {
        "@/test/*": ["../test/*"],
        "@/*": ["*"]
      }
    },
    "include": ["src", "test"]
}
